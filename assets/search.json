---
layout: null
---
[{% for post in site.wiki %}{
    "title":        "{{ post.title | escape }}",
    "discription":  "{{post.description | strip_html | strip_newlines | escape }}",
    "url":          "{{ site.baseurl }}{{ post.url }}",
    "category":     "{{ post.categories | join: ', '}}",
    "tags":         "{{ post.tags | join: ', ' }}",
    "date":         "{{ post.date }}"
}{% unless forloop.last %}, {% endunless %}{% endfor %}]